{
  "name": "updaterdemo",
  "version": "1.1.0",
  "description": "POC for electron-updater",
  "main": "./dist/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "tsc && electron .",
    "build": "tsc",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhavalc25/UpdaterDemo.git"
  },
  "build": {
    "appId": "updaterdemo1",
    "productName": "updaterdemo",
    "asar": true,
    "asarUnpack": [
      "node_modules/express/**"
    ],
    "directories": {
      "output": "build"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "dchauhan",
        "repo": "UpdaterDemo"
      }
    ],
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ],
      "category": "Utility"
    }
  },
  "keywords": [],
  "author": {
    "name": "Aunalytics",
    "email": "dhaval.chauhan@aunalytics.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dhavalc25/UpdaterDemo/issues"
  },
  "homepage": "https://github.com/dhavalc25/UpdaterDemo#readme",
  "dependencies": {
    "electron-updater": "^6.1.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.18",
    "electron": "^26.3.0",
    "electron-builder": "^24.6.4",
    "electron-rebuild": "^3.2.9",
    "express": "^4.18.2"
  }
}
